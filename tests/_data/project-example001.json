{
  "baseUri":"https://api.innnz.com",
  "version":"v2",
  "defaultMaxRetryCount":3,
  "defaultRetryInterval":"1000",
  "authentication":{
    "#type":"认证类型：HTTP_BASIC_AUTH,DIGEST, HTTP_SSL_CLIENT, OAUTH2，COOKIE_SESSION_AUTH(用户密码认证)，TOKEN_AUTH, JSON_WEB_TOEKN",
    "type":"OAUTH2",
    "grantType":"AUTHENTICATION_CODE",
    "expires":"7200"
  },
  "failureHttpStatusCode":"![200-299]",
  "failureResponse":{
    "requestId":{
      "type":"string",
      "typeClass":"",
      "name":"requestId",
      "nullable":false,
      "isAbandon":false,
      "default":""
    },
    "error":{
      "type":"Struct",
      "typeClass":"",
      "name":"",
      "nullable":false,
      "isAbandon":true,
      "default":{},
      "nesting":{
        "code":{
          "type":"string",
          "typeClass":"",
          "name":"code",
          "isNull":false,
          "isAbandon":false,
          "default":null
        },
        "message":{
          "type":"string",
          "typeClass":"",
          "name":"message",
          "isNull":false,
          "isAbandon":true,
          "default":null
        }
      }
    }
  },
  "apis":{
    "users":{
      "name":"",
      "api":"user",
      "method": "GET",
      "requestType":"json",
      "responseType":"json",
      "maxRetryCount":3,
      "retryInterval":"1000",
      "request":{
        "name":{
          "name":"name",
          "description":"",
          "type":"int8",
          "validator":"",
          "required":true,
          "isNull":false,
          "default":1
        }
      },
      "httpStatusCode":"200-299",
      "response":{
        "users": {
          "type":"[]User",
          "typeClass":"/namespace/User",
          "name":"userList",
          "nullable":false,
          "isAbandon":false,
          "default":[],
          "#nesting":"直接是User的属性信息",
          "nesting":{
            "lastName":{
              "type":"string",
              "typeClass":"",
              "name":"姓",
              "isNull":false,
              "isAbandon":false,
              "default":"unknown"
            },
            "firstName":{
              "type":"string",
              "typeClass":"",
              "name":"名",
              "isNull":false,
              "isAbandon":false,
              "default":"unknown"
            },
            "age":{
              "type":"int16",
              "typeClass":"",
              "name":"年龄",
              "isNull":false,
              "isAbandon":false,
              "default":18
            }
          }

        }
      }
    }
  }
}
